@model CinemaSchedule.Models.MovieViewModel;

<h1>Изменение фильма</h1>

<form method="post">
    <div class="mb-3">
        <label for="exampleFormControlInput1" class="form-label">Название заметки</label>
        <div class="col-sm-12">
            @Html.ValidationMessageFor(m => m.MovieName, null, new { @class = "text-danger" })
            <input type="text" class="form-control" asp-for="MovieName">
        </div>
    </div>

    <div class="mb-3">
        <label for="exampleFormControlTextarea1" class="form-label">Описание</label>
        <div class="col-sm-12">
            @Html.ValidationMessageFor(m => m.MovieDescription, null, new { @class = "text-danger" })
            <textarea class="form-control" rows="4" asp-for="MovieDescription"></textarea>
        </div>
    </div>
    <div class="mb-3">
        <label for="exampleFormControlTextarea1" class="form-label">Возрастное ограничение:</label>
        @Html.ValidationMessageFor(m => m.AgeRestriction, null, new { @class = "text-danger" })
