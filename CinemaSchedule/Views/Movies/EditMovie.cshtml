            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="AgeRestriction" value=0 asp-for="AgeRestriction" checked>
                <label class="form-check-label" for="inlineRadio1" id="age0">0+</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="AgeRestriction" value=6 asp-for="AgeRestriction">
                <label class="form-check-label" for="inlineRadio2" id="age6">6+</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="AgeRestriction" value=12 asp-for="AgeRestriction">
